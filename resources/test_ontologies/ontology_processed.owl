<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/fabad/ontologies/2023/8/untitled-ontology-961"
         xmlns="http://www.semanticweb.org/fabad/ontologies/2023/8/untitled-ontology-961#">

<owl:Ontology rdf:about="http://www.semanticweb.org/fabad/ontologies/2023/8/untitled-ontology-961"/>

<owl:ObjectProperty rdf:about="#actedIn">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <rdfs:domain rdf:resource="#Actor"/>
  <rdfs:range rdf:resource="#Film"/>
  <rdfs:comment>Relates an Actor to a Film in which they have acted.</rdfs:comment>
  <rdfs:label>acted in</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#belongsTo">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <rdfs:comment>Specifies the affiliation of an individual, such as a player, to a club or organization.</rdfs:comment>
  <rdfs:label>belongs to</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#wrote">
  <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
  <rdfs:domain rdf:resource="#Writer"/>
  <rdfs:range rdf:resource="#WrittenWork"/>
  <rdfs:comment>Relates a Writer to a WrittenWork they have authored.</rdfs:comment>
  <rdfs:label>wrote</rdfs:label>
</owl:ObjectProperty>

<owl:Class rdf:about="#Actor">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#actedIn"/>
          <owl:someValuesFrom rdf:resource="#Film"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Person"/>
  <rdfs:isDefinedBy rdf:resource="#Person"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">actor</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents individual who is actor, involved in acting in film.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Film">
  <rdfs:subClassOf rdf:resource="#ArtisticWork"/>
  <owl:disjointWith rdf:resource="#WrittenWork"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">film</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents film, specific type of artistic work typically presented in cinematic form.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Writer">
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#wrote"/>
      <owl:someValuesFrom rdf:resource="#WrittenWork"/>
    </owl:Restriction>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Person"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">writer</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents individual who writes, author, or creates written work.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#WrittenWork">
  <rdfs:subClassOf rdf:resource="#ArtisticWork"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">written work</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Denotes work that is written, such as book, article, or essay.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Person">
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">person</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents individual person.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#ArtisticWork">
  <rdfs:subClassOf rdf:resource="#Work"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">artistic work</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Denotes work of art, which is creative imaginative expression.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Work">
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">work</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents generic concept for work, which could include type of creative intellectual product.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#BasketballClub">
  <rdfs:subClassOf rdf:resource="#SportClub"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">basketball club</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents club specifically associated with basketball.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#SportClub">
  <rdfs:subClassOf rdf:resource="#Organization"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sport club</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents sport club, organization specifically associated with sport activity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#BasketballPlayer">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#belongsTo"/>
          <owl:someValuesFrom rdf:resource="#BasketballClub"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Sportsman"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">basketball player</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents individual who plays basketball and is affiliated with basketball club.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Sportsman">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#belongsTo"/>
          <owl:someValuesFrom rdf:resource="#SportClub"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Person"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sportsman</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents sportsman, individual associated with sport activity, such as playing for sport club.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Book">
  <rdfs:subClassOf rdf:resource="#WrittenWork"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">book</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents book, written work that is typically bound and consists of page, often containing literary content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#FootballClub">
  <rdfs:subClassOf rdf:resource="#SportClub"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">football club</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents football club, sport club specifically associated with football activity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#FootballPlayer">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#belongsTo"/>
          <owl:someValuesFrom rdf:resource="#FootballClub"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Sportsman"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">football player</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents football player, individual who plays football and is associated with football club.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Journal">
  <rdfs:subClassOf rdf:resource="#WrittenWork"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">journal</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents journal, written work that typically contains article, report, or scholarly news-related content.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Journalist">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#wrote"/>
          <owl:someValuesFrom rdf:resource="#Journal"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Writer"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">journalist</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents journalist, individual who writes or contributes to journalistic work such as article or report.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Novelist">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#wrote"/>
          <owl:someValuesFrom rdf:resource="#Book"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Writer"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">novelist</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents novelist, individual who writes and creates fictional book or novel.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Organization">
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">organization</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents organization, generic concept for group or entity formed for particular purpose or carrying out activity.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Poem">
  <rdfs:subClassOf rdf:resource="#WrittenWork"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">poem</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents poem, type of literary work characterized by expressive rhythmic quality, often in verse form.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Poet">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#wrote"/>
          <owl:someValuesFrom rdf:resource="#Poem"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Writer"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">poet</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents poet, individual who writes and creates poem.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#TennisClub">
  <rdfs:subClassOf rdf:resource="#SportClub"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tennis club</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents club specifically associated with tennis.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#TennisPlayer">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Person"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#belongsTo"/>
          <owl:someValuesFrom rdf:resource="#TennisClub"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Sportsman"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tennis player</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents individual who plays tennis and is affiliated with tennis club.</rdfs:comment>
</owl:Class>

<rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty">
  <rdfs:label>top object property</rdfs:label>
</rdf:Description>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Organization"/>
    <rdf:Description rdf:about="#Person"/>
    <rdf:Description rdf:about="#Work"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Journalist"/>
    <rdf:Description rdf:about="#Novelist"/>
    <rdf:Description rdf:about="#Poet"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Book"/>
    <rdf:Description rdf:about="#Journal"/>
    <rdf:Description rdf:about="#Poem"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#BasketballPlayer"/>
    <rdf:Description rdf:about="#FootballPlayer"/>
    <rdf:Description rdf:about="#TennisPlayer"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#BasketballClub"/>
    <rdf:Description rdf:about="#FootballClub"/>
    <rdf:Description rdf:about="#TennisClub"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Actor"/>
    <rdf:Description rdf:about="#Sportsman"/>
    <rdf:Description rdf:about="#Writer"/>
  </owl:members>
</owl:AllDisjointClasses>


</rdf:RDF>
